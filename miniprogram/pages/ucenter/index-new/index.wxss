page {
    min-height: 100%;
    background-color: #f4f4f4;
}
.container {
    width: 100%;
    background: #f4f4f4;
}

.userHeader {
    border-radius: 50%;
    overflow: hidden;
    width: 120rpx;
    height: 120rpx;
    position: relative;
    background: yellow;
    left: 0rpx;
    top: 0rpx;
}

.userbox {
    width: 100%;
    height: 300rpx;
    align-items: center;
    justify-content: center;
}
.userbox .userinfo {
    width: 100%;
    margin-top: 0rpx;
}

.order-area {width: 100%;margin-top:19rpx;margin-left:0rpx;background-color:#fff;}
.other-area {width: 100%;margin-top:19rpx;margin-left:0rpx;background-color:#fff;}
.arrow-right-image {
    width:32rpx;
    height:37rpx;
    float:left;
    margin-top:0rpx;
    margin-left:0rpx;
}


.entry-text{
    margin-top:0rpx;
    margin-left:0rpx;
    float:left;
    text-align:left;
    color:#000000;
    font-size:27rpx;
    line-height:42rpx;
}

.cut-line{
    background: #dbdbdb;
    width: 100%;
    height: 1rpx;
    line-height:10rpx;
}

.order-type-image{width:86rpx;height:57rpx;padding:0rpx;clear:both;margin-top:0rpx;margin-left:0rpx;float:left;text-align:left; line-height:57rpx;}
.order-type-name{width:86rpx;height:30rpx;padding:0rpx;clear:both;margin-top:12rpx;margin-left:0rpx;float:left;text-align:center; color:#949994;font-size:25rpx; line-height:30rpx;}

.ucenter-list-icon {
    width:43rpx;height:42rpx;padding:0rpx;margin-top:0rpx;margin-left:0rpx;float:left;text-align:left; line-height:42rpx;
}
.ucenter-list {
    width: 100%;
    height:42rpx;
    margin-top:30rpx;
    margin-left:32rpx;
}
